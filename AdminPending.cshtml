@model Team5Project.Models.Branch_Vehicle_Request
@{
    ViewBag.Title = "AdminPending";
}

@using (Html.BeginForm("RequestApprove", "Login"))
{
    <table border="1">
        <tr>
            <td>AS ON DATE</td>
            <td>VEHICLE ID</td>
            <td>NO OF VEHICLES</td>
            <td>BRANCH ID</td>
            <td>STATUS</td>

            <td>SELECT</td>
        </tr>
        @foreach (var i in ViewBag.L as IList<Team5Project.Models.Branch_Vehicle_Request>)
        {
            <tr>
                <td>@i.As_On_Date</td>
                <td>@i.Vehicle_id</td>
                <td>@i.No_of_Vehicles</td>
                <td>@i.Branch_id</td>
                <td>@i.Status</td>

                <td>@Html.CheckBox("chck", new { @value = i.Branch_id })</td>

            </tr>




        }

        <tr>
            <td><input type="submit" value="Approve" /></td>
        </tr>

    </table>
}



